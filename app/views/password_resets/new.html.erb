<% provide(:title, "密码重置")%>
<h1>密码重置</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:password_resets, url: password_resets_path) do |f| %>
     <%= f.label :email, "邮箱:" %>
     <%= f.text_field :email, class: "form-control" %>
     <%= f.submit "提交", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
